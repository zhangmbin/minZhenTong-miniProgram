.clinic-bgi {
  position: relative;
  top: 92rpx;
  left: 0;
}
.clinic {
  width: 750rpx;
  padding-top: 54rpx;
  padding-left: 30rpx;
  height: 351rpx;
  background: linear-gradient(180deg, #479AFB 0%, #0A7AFE 100%);
  display: flex;
  color: #fff;
  position: relative;
}
.clinic .clinic-header {
  z-index: 100;
  display: flex;
  padding-left: 24rpx;
  padding-bottom: 20rpx;
}
.clinic .clinic-header .clinic-info .clinic-name {
  font-size: 36rpx;
  font-weight: 700;
}
.clinic .clinic-header .clinic-info .clinic-address {
  font-size: 28rpx;
  color: #FFFFFF66;
  font-weight: 400;
  margin-top: 20rpx;
}
.clinic .clinic-header .clinic-info .state-icons {
  display: flex;
  margin-top: 12rpx;
}
.clinic .clinic-header .clinic-info .state-icons .state-online,
.clinic .clinic-header .clinic-info .state-icons .state-appointable {
  height: 36rpx;
  font-size: 24rpx;
  font-family: PingFang SC-Regular, PingFang SC;
  font-weight: 400;
  color: #ffffff;
  border-radius: 4rpx;
  padding-right: 12rpx;
  display: flex;
}
.clinic .clinic-header .clinic-info .state-icons .state-online {
  background: #E5AA16;
  margin-right: 20rpx;
}
.clinic .clinic-header .clinic-info .state-icons .state-online .state-online-icon {
