.clinic .clinic-header .clinic-bottom {
  display: flex;
  font-size: 26rpx;
  font-weight: 400;
  color: #ffffff66;
  margin-top: 6rpx;
}
.clinic .clinic-header .clinic-bottom .clinic-score text,
.clinic .clinic-header .clinic-bottom .consultation-num text {
  color: #fff;
  margin-left: 12rpx;
  margin-right: 32rpx;
}
.clinic-detail {
  width: 690rpx;
  height: 424rpx;
  position: absolute;
  top: 288rpx;
  left: 30rpx;
  padding: 30rpx;
}
.clinic-detail .consultation-tag {
  width: 132rpx;
  height: 40rpx;
  font-size: 28rpx;
  font-family: PingFang SC-Regular, PingFang SC;
  font-weight: 400;
  color: #fff;
  background: #0075FF;
  border-radius: 4rpx;
  padding-left: 10rpx;
}
.clinic-detail .time-available {
  margin-top: 12rpx;
  display: flex;
  flex-direction: column;
  font-size: 28rpx;
  font-family: PingFang SC-Regular, PingFang SC;
  font-weight: 400;
  color: #666666;
  line-height: 40rpx;
}
.clinic-detail .clinic-introduction {
  width: 100%;
  height: 202rpx;
  margin-top: 20rpx;
  font-size: 28rpx;
  font-family: PingFang SC-Regular, PingFang SC;
  font-weight: 400;
  color: #666666;
  line-height: 40rpx;
}
.clinic-detail .clinic-introduction .clinic-introduction-tag {
  float: left;
  margin-right: 18rpx;
}
.clinic-doctors {
  width: 690rpx;
  margin-left: 30rpx;
  padding: 30rpx 40rpx 30rpx 30rpx;
  position: absolute;
  top: 742rpx;
}
.clinic-doctors .doctor-header {
  font-size: 36rpx;
  font-family: PingFang SC-Heavy, PingFang SC;
  font-weight: 800;
  color: #333333;
}
.clinic-doctors .doctor-list {
  margin-top: 20rpx;
  display: flex;
  flex-wrap: wrap;
}
.clinic-doctors .doctor-list .doctor {
  display: flex;
  width: 155rpx;
  position: relative;
  flex-direction: column;
  font-family: PingFang SC-Regular, PingFang SC;
}
.clinic-doctors .doctor-list .doctor .doctor-name {
  font-size: 26rpx;
  font-weight: 800;
  color: #333333;
}
.clinic-doctors .doctor-list .doctor .medical-title {
  font-size: 22rpx;
  font-weight: 400;
  color: #999999;
}
.clinic-doctors .doctor-list .doctor .title {
  position: absolute;
  top: 0;
  left: 62rpx;
  background: linear-gradient(90deg, #FF836D 0%, #E7503D 100%);
  border-radius: 16rpx 16rpx 16rpx 16rpx;
  border: 2rpx solid #FFFFFF;
  color: #FFFFFF;
  font-size: 20rpx;
  font-family: PingFang SC-Heavy, PingFang SC;
  font-weight: 800;
  padding: 2rpx 8rpx 2rpx 12rpx ;
  z-index: 1;
}
